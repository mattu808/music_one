<h1><%= @artist.name %>: Rankings</h1>
<%= link_to 'Add Ranking', new_artist_ranking_path(@artist) %>
<hr />

<h2>Teacher</h2>
<ul>
  <% @teachers.each do |teacher| %>
    <li>
      <%= teacher.user.full_name %>
      <br />
      <%= link_to 'Remove', artist_ranking_path(@artist, teacher), method: :delete %>
    </li>
  <% end %>
</ul>
<h2>Ta</h2>
<ul>
  <% @tas.each do |ta| %>
    <li>
      <%= ta.user.full_name %>
      <br />
      <%= link_to 'Remove', artist_ranking_path(@artist, ta), method: :delete %>
    </li>
 <% end %>
</ul>
<h2>Student</h2>
<ul>
  <% @students.user.each do |student| %>
    <li>
      <%= student.user.full_name %>
      <br />
      <%= link_to 'Remove', artist_ranking_path(@artist, student), method: :delete %>
   </li>
 <% end %>
</ul>