<h1>Songs#new</h1>
<%= form_with(model: @song, local: true, class: "ui form") do |f| %>
  <div class="field">
    <%= f.label :artist %>
    <%= f.text_field :first_name, required: true, autofocus: true, placeholder: "First Name" %>
  </div>
  <div class="field">
    <%= f.label :song %>
    <%= f.text_field :song, required: true, placeholder: "Last Name" %>
  </div>
  <%= f.submit nil, class: "ui green button inverted" %>
<% end %>